<script type="text/javascript">
    $("#nv li").hover(function(){
        var index=$(this).index();
        $("#nv li.current").addClass("curr");
        if(index==0) return;
        $(".bar",this).show();
        $("#nv .libar").show();
        $(">a",this).addClass("hover");
    },function(){
        $("#nv li.current").removeClass("curr");  
        $(".bar",this).hide();
        $("#nv .libar").hide();
        $(">a",this).removeClass("hover");
    })
    
    $("#nv li:eq(0)").hover(function(){
        $("#nv li.current").addClass("curr");
    },function(){
        $("#nv li.current").removeClass("curr");  
    }) 
  </script>
